{
  "name": "listofjobs",
  "version": "1.0.0",
  "description": "Mentor's app",
  "main": "app.js",
  "homepage": "https://Andrew61-20.github.io/Interactive-Table",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "client": "npm run start --prefix client",
    "client:install": "npm install --prefix client",
    "client:build": "npm run build --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "predeploy": "npm run client:build",
    "deploy": "gh-pages -b master -d client:build"
  },
  "keywords": [
    "l-jobs",
    "react"
  ],
  "author": "Andrew",
  "license": "ISC",
  "dependencies": {
    "config": "^3.3.6",
    "express": "^4.17.1",
    "gh-pages": "^3.2.3",
    "mongodb": "^4.1.2"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "nodemon": "^2.0.12"
  }
}
